{
  "name": "exams",
  "version": "0.1.0",
  "private": true,
  "overrides": {
    "require-in-the-middle": "8.0.1",
    "import-in-the-middle": "2.0.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "NODE_OPTIONS='--require dd-trace/init' next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:changed": "eslint $(git diff --name-only --diff-filter=ACMR HEAD | grep -E '\\.(js|jsx|ts|tsx)$' | xargs)",
    "lint:staged": "eslint $(git diff --cached --name-only --diff-filter=ACMR | grep -E '\\.(js|jsx|ts|tsx)$' | xargs)",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:quick": "playwright test --reporter=list",
    "test:e2e:ci": "playwright test --reporter=html,github",
    "heroku-cleanup": "rm -rf .next/cache || true",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.2",
    "@ai-sdk/google-vertex": "^4.0.2",
    "@ai-sdk/react": "^3.0.5",
    "@heroicons/react": "^2.2.0",
    "@mui/material": "^6.3.0",
    "@next/third-parties": "^15.1.6",
    "@react-pdf-viewer/core": "^3.12.0",
    "@react-pdf-viewer/highlight": "^3.12.0",
    "@react-pdf-viewer/page-navigation": "^3.12.0",
    "@react-pdf-viewer/scroll-mode": "^3.12.0",
    "@react-pdf-viewer/thumbnail": "^3.12.0",
    "@sentry/nextjs": "^10.27.0",
    "@serwist/next": "^9.5.0",
    "@tanstack/react-query": "^5.90.11",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@tiptap/extension-typography": "^2.26.1",
    "@tldraw/tldraw": "^3.11.0",
    "agentation": "^1.1.1",
    "ai": "^6.0.5",
    "axios": "^1.7.9",
    "claude": "^0.1.1",
    "dd-trace": "^5.82.0",
    "firebase": "^11.1.0",
    "firebase-admin": "^13.0.2",
    "framer-motion": "^12.6.3",
    "html2canvas": "^1.4.1",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.552.0",
    "mathlive": "^0.104.2",
    "matter-js": "^0.20.0",
    "microsoft-cognitiveservices-speech-sdk": "^1.46.0",
    "modern-screenshot": "^4.6.6",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "next": "15.5.9",
    "next-auth": "^5.0.0-beta.25",
    "novel": "^1.0.2",
    "pdf-parse": "^1.1.4",
    "pdfjs-dist": "^3.11.174",
    "posthog-node": "^5.14.0",
    "raindrop-ai": "^0.0.59",
    "react": "^19.1.4",
    "react-dom": "^19.1.4",
    "react-fast-marquee": "^1.6.5",
    "react-markdown": "^9.0.1",
    "react-sketch-canvas": "^6.2.0",
    "recharts": "^3.2.1",
    "rehype-katex": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "remark-gemoji": "^8.0.0",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "remark-supersub": "^1.0.0",
    "resend": "^4.1.2",
    "serwist": "^9.5.0",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.0",
    "stripe": "^17.5.0",
    "tailwind-merge": "^3.3.1",
    "tiptap-markdown": "^0.3.4",
    "uuidv7": "^1.1.0",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/matter-js": "^0.19.8",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/parser": "^8.50.1",
    "@vitejs/plugin-react": "^5.1.1",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "^15.2.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8",
    "prettier": "^3.7.4",
    "prettier-plugin-organize-imports": "^4.3.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vitest": "^4.0.14"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  }
}